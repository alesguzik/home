Xcursor.theme: Adwaita

Emacs.Font: Input Mono ExLight-18

! XTerm*termName: xterm-256color
! URxvt*termName: rxvt-unicode-256color
URxvt*depth: 32
! URxvt*background: [90]#221100
URxvt*background: [100]#222222
URxvt*foreground: white
URxvt*scrollBar: false
URxvt*scrollTtyKeypress: true
! URxvt*cursorBlink: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*font: xft:InputMono Thin:size=18
URxvt*loginShell: false
URxvt*geometry: 110x35
URxvt*saveLines: 9999

URxvt*launcher: firefox
URxvt.matcher.button: 1
!URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,matcher
!URxvt.url-select.launcher: firefox
!URxvt.url-select.underline: true
!URxvt.keysym.M-u:      perl:url-select:select_next
!URxvt.keysym.M-Escape: perl:keyboard-select:activate
!URxvt.keysym.M-s:      perl:keyboard-select:search

rofi.font: NotoSans 16
rofi.color-enabled: true
rofi.color-normal:  argb:00000000,  #D8DEE9 , argb:00000000,  #FAC863 ,  #1B2B34
rofi.color-urgent:  argb:00000000,  #F99157 , argb:00000000,  #F99157 ,  #1B2B34
rofi.color-active:  argb:00000000,  #6699CC , argb:00000000,  #6699CC ,  #1B2B34
rofi.color-window:  argb:ee222222,  #FAC863 ,  #FAC863
rofi.separator-style:                solid

*.cursorColor: LightBlue
*.dynamicColors:  on
*.colorBDMode: off
*.colorULMode: off

[user]
	name = Ales Guzik
	email = me@aguzik.net
[giggle]
	main-window-maximized = false
	main-window-geometry = 958x1059+0+19
	file-view-vpane-position = 555
	main-window-view = HistoryView
	history-view-vpane-position = 541
[gui]
	spellingdictionary = none
[core]
	excludesfile = ~/.gitignore_global
[branch "master"]
	rebase = true
[alias]
	new = !cd "${GIT_PREFIX-.}" && git init && git commit --allow-empty -m root-commit
	co = checkout
	st = status
	s = status
	me = log --author='Ales Guzik' --format=format:'%ci %s'
	sub = submodule update --init --recursive
	staged = diff --staged
	unstage = reset HEAD
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[color]
	ui = auto
[push]
	default = current
[merge]
	defaultToUpstream = true
